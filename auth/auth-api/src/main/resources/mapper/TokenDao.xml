<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spring.auth.api.dao.TokenDao">
  
  <select id="findById" resultType="TokenVo">
    SELECT /* TokenDao.findById */
           id, token, client_ip, use_yn
    FROM   token
    WHERE  id = #{id}
  </select>
  
</mapper>
